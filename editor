static char	*set_line(char *line)
{
	int		i;
	char	*stash_r

	i = 0;
	while (line[i] != '\n' && line[i] != '\0')
		i++;
	if (line[i] == '\0' || line[i + 1] == '\0')
		return (NULL);
	free(stash_r);
	stash_r = ft_substr(line, i + 1, ft_strlen(line) - i);
	if (stash_r &&  *stash_r == 0)
	{
		free(stash_r);
		stash_r = NULL;
	}
	line[i + 1] = 0;
	return (stash_r);
}